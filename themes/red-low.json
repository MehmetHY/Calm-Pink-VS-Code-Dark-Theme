    "tokenColors": [
        {
            "name": "Keyword",
            "scope": [
                "keyword",
                "support.type.primitive.ts",
                "entity.name.tag.html",
                "keyword.operator.expression.in.ts",
                "keyword.operator.expression.of.ts",
                "keyword.operator.logical.python",
                "keyword.operator.wordlike.cpp",
                "keyword.operator.and.cpp",
                "keyword.operator.and_eq.cpp",
                "keyword.operator.bitand.cpp",
                "keyword.operator.bitor.cpp",
                "keyword.operator.compl.cpp",
                "keyword.operator.not.cpp",
                "keyword.operator.not_eq.cpp",
                "keyword.operator.or.cpp",
                "keyword.operator.or_eq.cpp",
                "keyword.operator.xor.cpp",
                "keyword.operator.xor_eq.cpp",
                "keyword.operator.typeid.cpp",
                "keyword.other.unit",
                "punctuation.definition.template-expression.begin.js",
                "punctuation.definition.template-expression.end.js"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#fda1a1"
            }
        },
        {
            "name": "String",
            "scope": ["string"],
            "settings": {
                "foreground": "#e3b08f"
            }
        },
        {
            "name": "Class",
            "scope": [
                "support.class",
                "support.type",
                "type",
                "entity.name.type.class.cpp",
                "entity.name.scope-resolution.function.definition.cpp",
                "entity.name.scope-resolution.cpp",
                "entity.name.type",
                "storage.type.cs",
                "entity.other.inherited-class",
                "variable.other.readwrite.alias.ts",
                "entity.other.ng-binding-name.template.html",
                "entity.name.operator.type.cpp",
                "meta.tag.sgml",
                "variable.other.readwrite.alias.js"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#96f8ac"
            }
        },
        {
            "name": "Built-in Type",
            "scope": [
                "storage.type",
                "storage",
                "entity.name.tag.structure.any.html",
                "entity.name.tag.inline.any.html",
                "entity.name.tag",
                "meta.selector.css",
                "keyword.operator.expression.typeof.js"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#e63ba4"
            }
        },
        {
            "name": "Variable",
            "scope": [
                "variable",
                "meta.block",
                "entity.name.variable",
                "source.glsl",
                "source.python",
                "source.kotlin",
                "meta.template.expression.kotlin",
                "variable.other.readwrite.js"
            ],
            "settings": {
                "foreground": "#a5c3e0"
            }
        },
        {
            "name": "Number",
            "scope": [
                "constant.numeric",
                "constant.language.boolean",
                "constant.language.json",
                "constant.numeric.css"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#ffef9c"
            }
        },
        {
            "name": "Function",
            "scope": [
                "meta.function",
                "entity.name.function.call",
                "support.function.glsl",
                "entity.name.function.cs",
                "entity.name.function",
                "meta.function-call.generic.python",
                "support.function.builtin.python",
                "entity.name.function.operator.member.cpp",
                "keyword.other.operator.overload.cpp",
                "entity.name.function.operator.cpp",
                "support.function.misc.css",
                "support.function.js",
                "support.function"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#ffda7d"
            }
        },
        {
            "name": "Function Definition",
            "scope": [
                "meta.head.function.definition",
                "entity.name.function.definition"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#7dffff"
            }
        },
        {
            "name": "PreProcessor",
            "scope": [
                "meta.preprocessor.include.cpp",
                "keyword.control.directive.include.cpp",
                "entity.name.function.preprocessor.cpp",
                "keyword.directive.preprocessor.glsl",
                "support.variable.magic.python",
                "support.constant"
            ],
            "settings": {
                "foreground": "#ffa7fe"
            }
        },
        {
            "name": "Enum",
            "scope": [
                "variable.other.enummember",
                "enumMember",
                "constant.other"
            ],
            "settings": {
                "foreground": "#65c96e"
            }
        },
        {
            "name": "Namespace",
            "scope": ["entity.name.namespace"],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#7676ff"
            }
        },
        {
            "name": "Comment",
            "scope": ["comment"],
            "settings": {
                "foreground": "#639666"
            }
        },
        {
            "name": "Documentation",
            "scope": ["comment.line.double-slash.documentation"],
            "settings": {
                "foreground": "#4bbd51"
            }
        },
        {
            "name": "Special Keywords",
            "scope": [
                "constant.language",
                "keyword.operator.new",
                "keyword.control.return",
                "keyword.operator.delete",
                "keyword.control.flow.return",
                "variable.language.this.ts",
                "keyword.operator.sizeof",
                "storage.modifier.glsl",
                "keyword.other.this",
                "variable.language.special.self.python",
                "variable.parameter.function.language.special.self.python",
                "keyword.operator.cast.static_cast.cpp",
                "keyword.operator.cast.cpp",
                "source.css.embedded.html",
                "entity.other.attribute-name.class.css",
                "entity.other.attribute-name.id.css"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#c570fa"
            }
        },
        {
            "name": "Special Modifiers",
            "scope": [
                "storage.modifier.virtual",
                "storage.modifier.specifier.functional.post-parameters.override",
                "keyword.other.set",
                "keyword.other.get"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#d2a7ff"
            }
        },
        {
            "name": "Special Access Modifiers",
            "scope": [
                "storage.modifier.static",
                "storage.type.modifier.access",
                "storage.modifier",
                "entity.name.tag.css"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#da5d86"
            }
        },
        {
            "name": "Punctuation and Operator",
            "scope": [
                "keyword.operator",
                "punctuation",
                "entity.name.function.operator",
                "storage.modifier.reference",
                "storage.modifier.pointer",
                "storage.type.function.arrow.ts",
                "meta.brace",
                "punctuation.definition.tag.html",
                "storage.type.function.arrow.js"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Property",
            "scope": [
                "entity.name.variable.property",
                "meta.object-literal.key",
                "variable.object.property.ts",
                "meta.definition.property.ts",
                "variable.other.property.ts",
                "variable.other.property",
                "support.variable.glsl",
                "support.type.property-name.json",
                "entity.other.attribute-name.html",
                "entity.name.variable.field.cs",
                "entity.name.variable.property.cs",
                "variable.other.object.property.cs",
                "variable.other.object.cs",
                "support.type.property-name.css",
                "variable.other.object.property.js"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#80d2ff"
            }
        },
        {
            "name": "Other",
            "scope": ["text.html.derivative", "text.html"],
            "settings": {
                "foreground": "#c4c4c4"
            }
        }
    ]
}
