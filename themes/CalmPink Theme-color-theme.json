	"tokenColors": [
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"support.type.primitive.ts",
				"entity.name.tag.html",
				"keyword.operator.expression.in.ts",
				"keyword.operator.expression.of.ts"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ff6f6f"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
			],
			"settings": {
				"foreground": "#e3b08f"
			}
		},
		{
			"name": "Class",
			"scope": [
				"support.class",
				"support.type",
				"type",
				"entity.name.type.class.cpp",
				"entity.name.scope-resolution.function.definition.cpp",
				"entity.name.scope-resolution.cpp",
				"entity.name.type",
				"storage.type.cs",
				"entity.other.inherited-class",
				"variable.other.readwrite.alias.ts",
				"entity.other.ng-binding-name.template.html"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#6fff8d"
			}
		},
		{
			"name": "Built-in Type",
			"scope": [
				"storage.type",
				"storage"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ff6f6f"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable",
				"meta.block",
				"entity.name.variable",
				"source.glsl"
			],
			"settings": {
				"foreground": "#8fb9e3"
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric",
				"constant.language.boolean",
				"constant.language.json"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ffe66f"
			}
		},
		{
			"name": "Function",
			"scope": [
				// "entity.name.function"
				"meta.function",
				"entity.name.function.call",
				"support.function.glsl",
				"entity.name.function.cs"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ffc735"
			}
		},
		{
			"name": "Function Definition",
			"scope": [
				"meta.head.function.definition",
				"entity.name.function.definition"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#36ffff"
			}
		},
		{
			"name": "PreProcessor",
			"scope": [
				"meta.preprocessor.include.cpp",
				"keyword.control.directive.include.cpp",
				"meta.block.cpp",
				"entity.name.function.preprocessor.cpp",
				"keyword.directive.preprocessor.glsl"
			],
			"settings": {
				"foreground": "#ff6ffd"
			}
		},
		{
			"name": "Enum",
			"scope": [
				"variable.other.enummember",
				"enumMember"
			],
			"settings": {
				"foreground": "#00dc11"
			}
		},
		{
			"name": "Namespace",
			"scope": [
				"entity.name.namespace"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#4b4bfc"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#639666"
			}
		},
		{
			"name": "Documentation",
			"scope": [
				"comment.line.double-slash.documentation"
			],
			"settings": {
				"foreground": "#4bbd51"
			}
		},
		{
			"name": "Special Keywords",
			"scope": [
				"constant.language",
				"keyword.operator.new",
				"keyword.control.return",
				"keyword.operator.delete",
				"keyword.control.flow.return",
				"variable.language.this.ts",
				"keyword.operator.sizeof",
				"storage.modifier.glsl"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#b236ff"
			}
		},
		{
			"name": "Special Modifiers",
			"scope": [
				"storage.modifier.virtual",
				"storage.modifier.specifier.functional.post-parameters.override",
				"keyword.other.set",
				"keyword.other.get"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#c38bff"
			}
		},
		{
			"name": "Special Access Modifiers",
			"scope": [
				"storage.modifier.static",
				"storage.type.modifier.access",
				"storage.modifier"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ffa661"
			}
		},
		{
			"name": "Punctuation and Operator",
			"scope": [
				"keyword.operator",
				"punctuation",
				"entity.name.function.operator",
				"storage.modifier.reference",
				"storage.modifier.pointer",
				"storage.type.function.arrow.ts",
				"meta.brace"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Property",
			"scope": [
				"entity.name.variable.property",
				"meta.object-literal.key",
				"variable.object.property.ts",
				"meta.definition.property.ts",
				"variable.other.property.ts",
				"variable.other.property",
				"support.variable.glsl",
				"support.type.property-name.json",
				"entity.other.attribute-name.html"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#36b7ff"
			}
		},
		{
			"name": "Other",
			"scope": [
				"text.html.derivative"
			],
			"settings": {
				"foreground": "#dddddd"
			}
		}
	]
}